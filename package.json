    "@types/koa-static": "^4.0.1",
    "@types/mongoose": "^5.5.0",
    "@types/node": "^12.0.0",
    "@types/uuid": "^3.4.4",
    "concurrently": "^4.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "preact-cli": "^3.0.0-rc.1",
    "sass-loader": "^7.1.0",
    "ts-loader": "^6.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-node-externals": "^1.7.2"
  }
