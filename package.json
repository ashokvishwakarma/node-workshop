{
  "name": "node-workshop",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run server\" \"npm run front\"",
    "server": "nodemon --watch ./src/server -e ts --exec \"ts-node --project ./tsconfig.server.json ./src/server/index.ts\"",
    "front": "preact watch --src ./src/front/index.ts --port 4001 --host 0.0.0.0",
    "build:server": "webpack --config ./webpack.server.js --output-path ./build/server -p",
    "build:front": "preact build --src ./src/front/index.ts --dest ./build/front --template ./src/front/index.html -p --no-service-worker true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashokvishwakarma/node-workshop.git"
  },
