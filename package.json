  },
  "homepage": "https://github.com/ashokvishwakarma/node-workshop#readme",
  "dependencies": {
    "dayjs": "^1.8.14",
    "dotenv": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-router": "^7.4.0",
    "mongoose": "^5.5.7",
    "preact": "^8.4.2",
    "preact-router": "^2.6.1",
    "redux-zero": "^5.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
